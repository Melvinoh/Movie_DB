n    CrewID INT,\n    DirectorsID INT,\n    CONSTRAINT fk_crewDirectors_crewTB FOREIGN KEY (CrewID) REFERENCES crew_tb(CrewID),\n    CONSTRAINT fk_crewDirectors_DirectorsTB FOREIGN KEY (DirectorsID) REFERENCES directors_tb(DirectorsID)\n);\n\nSHOW TABLES;\n\nSELECT TABLE_NAME, CONSTRAINT_TYPE, CONSTRAINT_NAME\nFROM information_schema.TABLE_CONSTRAINTS\nWHERE TABLE_SCHEMA = 'movieDB';\n\nINSERT into language_tb (languageName) values ('english');\nINSERT INTO genre_tb (GenreName) VALUES\n('Action'),\n('Drama'),\n('Comedy');\n\nINSERT INTO actors_tb (Firstname, Lastname, Nationality, EmailAddress, gender) VALUES\n('Leonardo', 'DiCaprio', 'American', 'leo.dicaprio@example.com', 'M'),\n('Marlon', 'Brando', 'American', 'marlon.brando@example.com', 'M'),\n('Bong', 'Joon', 'Korean', 'bong.joon@example.com', 'M'),\n('Christian', 'Bale', 'British', 'christian.bale@example.com', 'M'),\n('Audrey', 'Tautou', 'French', 'audrey.tautou@example.com', 'F'),\n('Al', 'Pacino', 'American', 'al.pacino@example.com', 'M'),\n('Anne', 'Hathaway', 'American', 'anne.hathaway@example.com', 'F'),\n('Morgan', 'Freeman', 'American', 'morgan.freeman@example.com', 'M'),\n('Emma', 'Watson', 'British', 'emma.watson@example.com', 'F'),\n('Tom', 'Hardy', 'British', 'tom.hardy@example.com', 'M');\n\nINSERT INTO directors_tb (Firstname, Lastname, Nationality, EmailAddress, gender) VALUES\n('Christopher', 'Nolan', 'British-American', 'chris.nolan@example.com', 'M'),\n('Francis', 'Coppola', 'American', 'francis.coppola@example.com', 'M'),\n('Bong', 'Joon-ho', 'Korean', 'bong.joonho@example.com', 'M'),\n('James', 'Cameron', 'Canadian', 'james.cameron@example.com', 'M'),\n('Patty', 'Jenkins', 'American', 'patty.jenkins@example.com', 'F'),\n('Jean-Pierre', 'Jeunet', 'French', 'jean.jeunet@example.com', 'M');\n\nINSERT INTO ratings_tb (RatingNumber, sauerce) VALUES\n(4.5, 'IMDb'),\n(3.8, 'Rotten'),\n(4.0, 'Metacritic');\n\nINSERT into language_tb (languageName) values ('english');\nINSERT into language_tb (languageName) values ('spanish');\nINSERT into language_tb (languageName) values ('italiano');\n\nINSERT INTO crew_tb (CrewID) VALUES\n(1),\n(2),\n(3);\n\nINSERT INTO Crew_Actors (CrewID, ActorsID) VALUES\n(1, 1), -- Robert Downey in Avengers\n(1, 2), -- Scarlett Johansson in Avengers\n(2, 1), -- Robert Downey in Titanic (for example)\n(3, 4), -- Chris Hemsworth in Inception\n(3, 2), -- Chris Hemsworth in Inception\n(2, 1); -- Chris Hemsworth in Inception\n\n\n\nINSERT INTO Crew_Directors (CrewID, DirectorsID) VALUES\n(1, 1), -- Steven Spielberg directed Avengers\n(2, 1), -- James Cameron directed Titanic\n(3, 2), -- Patty Jenkins directed Inception\n(3, 1); -- Patty Jenkins directed Inception\nINSERT INTO movies_tb (crewID, languageID, movieTitle, releaseYear, description, ratings, movieLength) VALUES\n(1, 1, 'Avatar', '2009', 'Sci-fi epic movie', 2, 162),\n(2, 2, 'The Godfather', '1972', 'Crime drama masterpiece', 3, 175),\n(3, 3, 'Parasite', '2019', 'Thriller and social commentary', 1, 132),\n(2, 1, 'The Dark Knight', '2008', 'Superhero crime thriller', 1, 152),\n(1, 1, 'Avengers', '2012', 'Superhero action movie', 1, 143),\n(2, 2, 'Titanic', '1997', 'Romantic drama', 2, 195),\n(3, 3, 'Inception', '2010', 'Sci-fi thriller', 3, 148),\n(1, 3, 'Am√©lie', '2001', 'Romantic comedy-drama', 2, 122);\n\nselect * from movies_tb;\nSELECT * FROM actors_tb;\n\nSELECT * FROM directors_tb;\n\nSELECT * FROM ratings_tb;\nSELECT m.movieTitle, g.GenreName\nFROM Movie_Genre mg\nJOIN movies_tb m ON mg.MovieID = m.movieID\nJOIN genre_tb g ON mg.GenreID = g.GenreID;\n\nSELECT c.CrewID, a.Firstname, a.Lastname, m.movieTitle \nFROM Crew_Actors ca\nJOIN crew_tb c ON ca.CrewID = c.CrewID\nJOIN movies_tb m ON m.crewID = c.CrewID\nJOIN actors_tb a ON ca.ActorsID = a.ActorsID;\n\nselect m.movieTitle, r.RatingNumber, m.releaseYear\nfrom movies_tb m\njoin ratings_tb r on m.ratings = r.RatingsID;\nselect mg.MovieID, m.movieTitle, g.GenreName,'m.description'\nfrom Movie_Genre mg\njoin  movies_tb m on  mg.MovieID = m.movieID\njoin genre_tb g on mg.GenreID = g.GenreID;\ndesc genre_tb;\ndesc Movie_Genre;\n",
